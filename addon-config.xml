<!-- 
use this addon-config.xml to create/set/publish your property tree
or override default settings from defaults.xml

Node: this file MUST be named addon-config.xml and it MUST be in the
root path of your addon/plugin.
The topmost element MUST be <PropertyList>
-->

<PropertyList>
	<addons>
		<by-id>
			<org.flightgear.addons.HEMSGen>
				<settings>
					<min-distance-km type="int">2</min-distance-km>
					<max-distance-km type="int">60</max-distance-km>
					<type-probability type="float">0.5</type-probability>
					<guide-interval type="float">5.0</guide-interval>
				</settings>
			</org.flightgear.addons.HEMSGen>
		</by-id>
	</addons>
</PropertyList>
